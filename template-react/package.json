{
	"name": "template",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"check:js": "biome lint --write",
		"check:css": "stylelint \"**/*.{css,scss,sass,less,postcss}\" --fix",
		"check:style": "prettier --write \"**/*.{html,vue,xml,yml,yaml,css,scss,sass,less,postcss}\" && biome format --write",
		"check:all": "npm run check:js && npm run check:css && npm run check:style",
		"prepare": "husky",
		"lintstaged": "lint-staged",
		"commitlint": "commitlint --edit"
	},
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.19",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"postcss": "^8.4.39",
		"postcss-nested": "^6.0.1",
		"postcss-preset-env": "^9.5.15",
		"prettier": "^3.3.2",
		"stylelint": "^16.6.1",
		"stylelint-config-standard": "^36.0.1",
		"typescript": "^5.5.3",
		"vite": "^5.3.3"
	}
}
