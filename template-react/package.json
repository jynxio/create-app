{
    "name": "template",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "packageManager": "pnpm@*",
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint:js": "eslint \"**/*.{js,mjs,cjs,ts,jsx,tsx}\" --fix",
        "lint:css": "stylelint \"**/*.{css,scss,sass,less,postcss}\" --fix",
        "lint:style": "prettier --write .",
        "lint:all": "pnpm lint:js && pnpm lint:css && pnpm lint:style",
        "prepare": "husky",
        "lintstaged": "lint-staged",
        "commitlint": "commitlint --edit"
    },
    "dependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.0",
        "@commitlint/config-conventional": "^19.6.0",
        "@eslint/js": "^9.17.0",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.2",
        "@typescript-eslint/eslint-plugin": "^8.18.0",
        "@typescript-eslint/parser": "^8.18.0",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "globals": "^15.13.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "postcss": "^8.4.49",
        "postcss-preset-env": "^10.1.2",
        "prettier": "^3.4.2",
        "sass": "^1.83.0",
        "stylelint": "^16.11.0",
        "stylelint-config-standard": "^36.0.1",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.0",
        "vite": "^6.0.3"
    }
}
