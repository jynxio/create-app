{
    "extends": ["stylelint-config-standard", "stylelint-config-standard-vue"],
    "rules": {
        "property-no-unknown": [
            true,
            {
                "ignoreProperties": ["composes"]
            }
        ],
        "selector-pseudo-class-no-unknown": [
            true,
            {
                "ignorePseudoClasses": ["global", "local"]
            }
        ]
    }
}
